{
    "env": {
        "SPI_FLASH_SIZE_MAP": "${config:esp.SPI_FLASH_SIZE_MAP}"
      },
    "configurations": [
        {
        
            "name": "esp8266", 
            "includePath": [
                "${workspaceFolder}/**",
                "${config:esp.path.Sdk}/**",
                "${config:esp.path.compiler}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "SPI_FLASH_SIZE_MAP=${SPI_FLASH_SIZE_MAP}" 
            ],
            "cStandard": "c89",
            "intelliSenseMode": "windows-gcc-x64",
            "compilerPath": "${config:esp.path.compiler}\\bin\\xtensa-lx106-elf-gcc" 
        }
    ],
    "version": 4
}